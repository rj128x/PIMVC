<?xml version="1.0" encoding="utf-8" ?>
<root xmlns="reportSchema.xsd">
	<nameReport>Масло</nameReport>
	<expReplace>
		<replace key="~LKotel~"
					value="VotGES.GA9.MNU.Lkotel"/>
		<replace key="~LBak~"
					value="VotGES.GA9.MNU.Lbak"/>
		<replace key="~LLA~"
					value="VotGES.GA9.Leak.L"/>
		<replace key="~PKotel~"
					value="VotGES.GA9.MNU.Pkotel"/>
		<replace key="~TKotel~"
					value="VotGES.GA9.MNU.Tкотел"/>
		<replace key="~TBak~"
					value="VotGES.GA9.MNU.Tbak"/>
		<replace key="~P~"
					value="VotGES.GA9.P"/>
		<replace key="~VBak~"
					value="(('VotGES.GA9.MNU.Lbak'+600)/1000*12)"/>
		<replace key="~VKotel~"
					value="(('VotGES.GA9.MNU.Lkotel'+1620)/1000*5.641)"/>
		<replace key="~VLA~"
					value="('VotGES.GA9.Leak.L')/1000*0.03/0.138"/>
		<replace key="~VSUM~"
					value="(~VBak~+~VKotel~+~VLA~)"/>
	</expReplace>
	<items>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Lkotel"
				title="Уровень в котле"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Lbak"
				title="Уровень в баке"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Lkotel"
				title="VotGES.GA9.Leak.L"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Pkotel"
				title="Давление в котле"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Tкотел"
				title="Температура в котле"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.MNU.Tbak"
				title="Температура в баке"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="tag"
				tagName="VotGES.GA9.P"
				title="Нагрузка"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				toChart="true"/>
		<item mode="expr"
				expr="~VBak~"
				title="Объем в баке"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				sampleType="stInterval"
				sampleInterval="10s"
				toChart="true"/>
		<item mode="expr"
				expr="~VKotel~"
				title="Объем в котле"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				sampleType="stInterval"
				sampleInterval="10s"
				toChart="true"/>
		<item mode="expr"
				expr="~VLA~"
				title="Объем в ЛА"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				sampleType="stInterval"
				sampleInterval="10s"
				toChart="true"/>
		<item mode="expr"
				id="vsum"
				expr="~VSUM~"
				title="Объем"
				summariesType="asAverage"
				calculationBasis="cbTimeWeighted"
				sampleType="stInterval"
				sampleInterval="10s"
				toChart="true"/>
	</items>
</root>